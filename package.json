{
  "name": "vimeoplayer",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build && npm run sentry:sourcemaps",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org vikki-ii --project javascript-angular ./dist && sentry-cli sourcemaps upload --org vikki-ii --project javascript-angular ./dist"
